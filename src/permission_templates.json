        },
        {
            "key": "codeviewer",
            "name": "See Source Code",
            "description": "View the project's source code (for private projects, users also need the \"Browse\" permission)"
        },
        {
            "key": "issueadmin",
            "name": "Administer Issues",
            "description": "Resolve issues as being \"fixed\", \"accepted\" or \"false-positive\" (for private projects, users also need the \"Browse\" permission)."
        },
        {
            "key": "securityhotspotadmin",
            "name": "Administer Security Hotspots",
            "description": "Resolve a Security Hotspot as reviewed (fixed or safe), reset it as to review (for private projects, users also need the \"Browse\" permission)."
        },
        {
            "key": "scan",
            "name": "Execute Analysis",
            "description": "Ability to get all settings required to perform an analysis (including the secured settings like passwords) and to push analysis results to the {instance} server."
        },
        {
            "key": "user",
            "name": "Browse",
            "description": "Access a project, browse its measures and issues, confirm issues, change the assignee, comment on issues and change tags."
        }
